    "def get_soup(url):\n",
    "    driver = webdriver.Firefox()\n",
    "    driver.get(url)\n",
    "    time.sleep(2)\n",
    "    bs = BeautifulSoup(driver.page_source)\n",
    "    driver.close()\n",
    "    return bs"
